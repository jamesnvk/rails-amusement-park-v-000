<h1> New User </h1>
<%= form_for @user do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br/>
  <%= f.label :height %>
  <%= f.text_field :height %><br/>
  <%= f.label :happiness %>
  <%= f.text_field :happiness %><br/>
  <%= f.label :nausea %>
  <%= f.text_field :nausea %><br/>
  <%= f.label :tickets %>
  <%= f.text_field :tickets %><br/>
  <p>Check box if you are a theme park owner <%= f.check_box :admin %></p>
  <%= f.submit %>
<% end %>